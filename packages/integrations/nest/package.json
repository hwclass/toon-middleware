{
  "name": "@toon-middleware/nest",
  "version": "1.0.0",
  "description": "NestJS module for TOON format - Automatic token optimization for LLM API responses",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "nestjs",
    "middleware",
    "interceptor",
    "toon",
    "llm",
    "token-optimization",
    "api-optimization",
    "cost-reduction",
    "typescript"
  ],
  "author": "hwclass",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/toon-format/toon-middleware.git",
    "directory": "packages/integrations/nest"
  },
  "bugs": {
    "url": "https://github.com/toon-format/toon-middleware/issues"
  },
  "homepage": "https://github.com/toon-format/toon-middleware/tree/main/packages/integrations/nest#readme",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "npm run build && node --test dist-test/tests",
    "test:coverage": "npm run build && node --test --experimental-test-coverage dist-test/tests",
    "test:watch": "tsc -p tsconfig.test.json --watch",
    "lint": "eslint src/ tests/",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist dist-test",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@toon-middleware/core": "workspace:*",
    "@toon-middleware/utils": "workspace:*",
    "@toon-middleware/cache": "workspace:*",
    "@toon-middleware/logger": "workspace:*"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "rxjs": "^7.0.0",
    "reflect-metadata": "^0.1.0 || ^0.2.0"
  },
  "devDependencies": {
    "@nestjs/common": "^10.3.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/testing": "^10.3.0",
    "@types/express": "^4.17.0",
    "@types/node": "^24.0.0",
    "@types/supertest": "^6.0.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.0",
    "supertest": "^7.0.0",
    "typescript": "^5.0.0"
  }
}
